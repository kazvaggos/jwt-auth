{
  "name": "jwt-auth",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "db:migrate": "knex migrate:latest",
    "start": "npm run prod",
    "debug": "DEBUG=* babel-node --inspect ./src/bin/www",
    "server": "babel-node ./src/bin/www",
    "server:prod": "node ./dist/bin/www",
    "dev": "NODE_ENV=development npm-run-all server",
    "dev:watch": "nodemon",
    "clean": "rimraf ./dist",
    "build:server": "babel ./src --out-dir ./dist",
    "prod": "NODE_ENV=production npm-run-all clean build:server server:prod",
    "docs": "apidoc -i src -o reports/apidoc",
    "test": "jest -c jest/config.unit.js --setupFiles dotenv/config",
    "test:coverage": "jest -c jest/config.unit.js --setupFiles dotenv/config --coverage",
    "test:functional": "jest -c jest/config.fun.js --setupFiles dotenv/config",
    "docker:build": "docker build -t jwt-auth/api ."
  },
  "dependencies": {
    "bcrypt": "^3.0.6",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^2.4.0",
    "morgan": "~1.9.1",
    "objection": "^1.6.11",
    "pg": "^7.12.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/node": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "apidoc": "^0.17.7",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "eslint": "^5.6.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "jest": "^24.9.0",
    "jest-html-reporter": "^2.6.2",
    "node-mocks-http": "^1.8.0",
    "nodemon": "^1.19.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.14.3",
    "rimraf": "^3.0.0",
    "supertest": "^4.0.2"
  }
}
